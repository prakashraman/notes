<div class='notes'>
  {{#each notes}}
    <article>
      <div class="content-wrapper">
        <h2>{{title}}</h2>
        <small class='date'>{{publishedAt}}<span class='dot'>&middot;</span> Tech</small>
        <p>{{content}}</p>
        <p><a href='{{url}}'>read more</a></p>
      </div>
    </article>
  {{/each}}  
</div>
